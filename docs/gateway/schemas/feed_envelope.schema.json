{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Canonical Feed Envelope",
  "type": "object",
  "required": ["data", "source_timestamp", "quality_flags", "warnings"],
  "properties": {
    "data": { "type": "object" },
    "source_timestamp": { "type": "string", "format": "date-time" },
    "quality_flags": {
      "type": "array",
      "items": { "type": "string" }
    },
    "warnings": {
      "type": "array",
      "items": { "type": "string" }
    }
  },
  "additionalProperties": true
}
